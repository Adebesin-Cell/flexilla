{
  "name": "@flexilla/flexilla",
  "private": false,
  "version": "1.1.0",
  "type": "module",
  "description": "Flexilla is an open-source set of unstyled interactive UI components for building interactive, and customizable user interfaces.",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib.d.ts",
        "default": "./dist/lib.js"
      },
      "require": {
        "types": "./dist/lib.d.ts",
        "default": "./dist/lib.umd.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "!vite.config.ts",
    "!.gitignore",
    "!tsconfig.json",
    "!biome.js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && cat src/types-declaration/*.d.ts > dist/index.d.ts",
    "preview": "vite preview",
    "format": "biome format --write ./src"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.1",
    "@flexilla/accordion": "^1.1.0",
    "@flexilla/auto-resize-area": "^1.1.0",
    "@flexilla/collapse": "^1.1.0",
    "@flexilla/custom-range": "^1.1.0",
    "@flexilla/dismissible": "^1.1.0",
    "@flexilla/dropdown": "^1.1.0",
    "@flexilla/modal": "^1.1.0",
    "@flexilla/offcanvas": "^1.1.0",
    "@flexilla/tabs": "^1.1.0",
    "@flexilla/tooltip": "^1.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "keywords": [
    "Flexilla",
    "interactive component",
    "components library",
    "ts",
    "js",
    "ui component",
    "interactive"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/unify-ui-dev/flexilla"
  },
  "bugs": {
    "url": "https://github.com/unify-ui-dev/flexilla/issues"
  },
  "license": "MIT",
  "author": "johnkat-mj"
}
