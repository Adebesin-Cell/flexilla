{
  "name": "fx-lib",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "Flexilla is an open-source set of unstyled interactive UI components for building interactive, and customizable user interfaces.",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types":"./dist/lib.d.ts",
        "default":"./dist/lib.js"
      },
      "require": {
        "types":"./dist/lib.d.ts",
        "default":"./dist/lib.umd.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "!vite.config.ts",
    "!.gitignore",
    "!tsconfig.json",
    "!biome.js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "biome format --write ./src"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0",
    "@biomejs/biome": "1.5.1",
    "@fx-lib/accordion": "^1.0.0",
    "@fx-lib/auto-resize-area": "^1.0.0",
    "@fx-lib/collapse": "^1.0.0",
    "@fx-lib/custom-range": "^1.0.0",
    "@fx-lib/dismissible": "^1.0.0",
    "@fx-lib/dropdown": "^1.0.0",
    "@fx-lib/modal": "^1.0.0",
    "@fx-lib/offcanvas": "^1.0.0",
    "@fx-lib/tabs": "^1.0.0",
    "@fx-lib/tooltip": "^1.0.0"
  },
  "keywords": [
    "Flexilla",
    "fx-lib",
    "interactive component",
    "components library",
    "ts",
    "js",
    "collapsible",
    "component",
    "interactive",
    "intercative component"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/unify-ui-dev/flexilla"
  },
  "bugs": {
    "url": "https://github.com/unify-ui-dev/flexilla/issues"
  },
  "license": "MIT",
  "author": "johnkat-mj"
}
