{
  "name": "@fx-lib/modal",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "description": "A customizable modal component for creating interactive pop-up dialogs in web applications.",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/modal.umd.js",
  "module": "./dist/modal.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/modal.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/modal.umd.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "!vite.config.ts",
    "!.gitignore"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0"
  },
  "dependencies": {
    "@fx-lib/utilities": "^0.0.1"
  },
  "keywords": [
    "modal",
    "overlay",
    "dialog",
    "alert-dialog",
    "js",
    "ts",
    "component",
    "interactive",
    "intercative component"
  ],
  "homepage": "",
  "repository": {
    "type": "",
    "url": ""
  },
  "bugs": {
    "url": "https://github.com/unify-ui-dev"
